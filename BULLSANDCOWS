import random
#GateMaster must select a three digit code without duplicates
#KeyMaster has 10 tries total
Input1 = random.randint(0, 9)
Input2 = random.randint(0, 9)
Input3 = random.randint(0, 9)

Tries = 10
Guess = False

if Input1 == Input2 or Input2 == Input3 or Input1 == Input3:
    Input1 = random.randint(0, 9)
    Input2 = random.randint(0, 9)
    Input3 = random.randint(0, 9)
    

    
